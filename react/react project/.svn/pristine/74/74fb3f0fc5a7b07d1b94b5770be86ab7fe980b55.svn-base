
@media all
{
html{ font-size:20px; }
}

@media only screen and (min-device-width:360px) and (max-device-width:390px)
{
    html{ font-size:20px; }
}
@media only screen and (min-device-width:310px) and (max-device-width:330px)
{
    html{ font-size:17px; }
}
@media only screen and (min-device-width:400px) and (max-device-width:430px)
{
    html{ font-size:22px; }
}


