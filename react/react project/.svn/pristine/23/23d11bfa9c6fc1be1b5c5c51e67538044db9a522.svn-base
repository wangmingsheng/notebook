@import "media.css";
@import "base.css?d";


h2{font-size: 20px; }
h3{font-size: 18px; }
h4{font-size: 14px; }
h5{font-size: 12px; }
h6{font-size: 10px; }

/*上推对话框*/
.pushup_dialog{ position: absolute; width: 100%; height: 0; left: 0; bottom: 0; transition: height .5s;z-index: 10;background-color: #f2f2f2;}
.pushup_dialog.out{ height: 100%;}

.clearfix:after{ content:""; display:table; clear:both;}

/*-- Loading --*/
.loading_bounce{ margin:0 auto; width:80px; text-align:center; position:absolute; top:40%; left:0; right:0;}
.loading_bounce>div{ width:16px; height:16px; background-color:#ccc; border-radius:100%; display:inline-block; -webkit-animation:three-bounce 1.4s ease-in-out 0s infinite both; animation:three-bounce 1.4s ease-in-out 0s infinite both; margin:0 5px;}
.loading_bounce.small>div{ width:10px; height:10px; margin:0 3px;}
.loading_bounce>div:nth-child(1){ -webkit-animation-delay:-0.32s; animation-delay:-0.32s;}
.loading_bounce>div:nth-child(2){ -webkit-animation-delay:-0.16s; animation-delay:-0.16s;}
@-webkit-keyframes three-bounce {
    0%, 80%, 100% { -webkit-transform:scale(0);}
    40% { -webkit-transform:scale(1);}
}
@keyframes three-bounce {
    0%, 80%, 100% { transform:scale(0);}
    40% { transform:scale(1);}
}

/*-- CheckBox --*/
.checkbox { width: 21px; height: 21px;}
.checkbox input[type=checkbox]{ display:none; overflow:hidden;}
.checkbox input[type=checkbox] + i{ position:relative; color:#474747; padding-left:28px; font-size:14px; line-height:20px; white-space: nowrap;}
.checkbox input[type=checkbox] + i:after{ position:absolute; left: 0; top: 0; width:21px; height:21px; border-radius:3px; border:1px solid #bbb; content:""; overflow:hidden; z-index:0;}
.checkbox input[type=checkbox]:checked + i:after{ background:#1F75D2; border:1px solid #1F75D2;}
.checkbox input[type=checkbox]:checked + i:before{ content:"\e605"; position:absolute; left:2px; top:0; z-index:1; text-indent:0; font-weight:bold; color:#90BBE9; font-size:16px;}
.checkbox input[type=checkbox]:indeterminate + i:before{ content:""; width:10px; height:2px; overflow:hidden; background:#888888; position:absolute; left:3px; top:45%;}
.checkbox input[type=checkbox]:disabled + i{ cursor:default;}
.checkbox input[type=checkbox]:disabled + i:before{ color:#cccccc;}
.checkbox input[type=checkbox]:disabled + i:after{ border:1px solid #dddddd;}
.checkbox_radio{ display:inline-block;}
.checkbox_radio input[type=checkbox] + i:after{ border-radius:50%; }
.checkbox_radio input[type=checkbox]:checked + i:after{ border-radius:50%;}

.checkbox_round input[type=checkbox] + i:after{ border-radius:50%;}
/*SwitchBox*/
.switch_button{display: inline-block;}
.switch_button>input{ display: none; overflow: hidden;}
.switch_button>i{position: relative; display: inline-block; width: 51px; height: 31px; margin-top: 10px; }
.switch_button>i:before{ content: ''; position: absolute; left: 0; top: 0; width: 100%; height: 100%; background-color: #efeff4; border-radius: 15px; }
.switch_button>input:checked+i::before{ background-color: #1f75d2;}
.switch_button>i:after{ content: ''; position: absolute; width: 28px; height: 28px; top: 2px; right: 20px; border-radius: 50%; background-color: #ffffff; transition: right .3s;}
.switch_button>input:checked+i::after{ right: 2px;}

/*选择列表*/
.select_list{ width: 100%; }
.select_list>h3{ overflow: hidden;text-indent: 18px;}
.select_list>h3>span{ color: #9d9d9f; line-height: 36px; }
.select_list>ul{ position: relative; width: 100%; background-color: #ffffff;overflow: hidden;}
.select_list>ul>li{ position: relative; width: 515px;}
.select_list>ul>li::after{ content: ''; position: absolute; left: 0; right: 0; bottom: 0; border-bottom:1px solid #efeff4;/*no*/}
.select_list.short_line>ul>li:nth-child(1)::after{ left: 18px;}
.select_list>ul>li>.check{ display: inline-block; vertical-align: top; width: 46px; padding: 12px 0 12px 18px;}

.select_list>ul>li>.text{ position: relative; display: inline-block; width: 329px; padding: 12px 22px 12px 0;}
.select_list>ul>li>.text>.top_block{ line-height: 23px;}
.select_list>ul>li>.text>.top_block>.text{ font-size: 16px; color:#474747;}
.select_list>ul>li>.text>.top_block>.tip{ display: inline-block; height: 21px; padding: 0 8px 0 8px; margin-left: 10px; border-radius: 10px; font-size: 12px; text-align: center; color: #999999; background-color: #f2f2f2; }
.select_list>ul>li>.text>.top_block>.tip.later{ color: #ff8f78;background-color: #ffd4d3;}
.select_list>ul>li>.text>.bottom_block{ width: 100%; line-height: 16px;}
.select_list>ul>li>.text>.bottom_block>.brand{ font-size: 12px; line-height: 16px; color: #aaaaaa;}
.select_list>ul>li>.text>.touch_area{position: absolute; top: 0; right: 0; height: 100%; width: 280px;}
.select_list>ul>li>.option{ position: absolute; left: 375px; top: 0; width: 140px; height: 100%;}
.select_list>ul>li>.option>input[type='button']{display: inline-block; width: 70px; height: 100%; background-color: #b79de1; font-size: 26px; color: #ffffff;}
.select_list>ul>li>.option>input[type='button']:nth-child(1){ background-color: #b79de1}
.select_list>ul>li>.option>input[type='button']:nth-child(2){ background-color: #82ced1}
.select_list>ul>li>.option>input:nth-child(3){ position: absolute; right: 370px; top: 0; width: 1px; height: 1px;z-index: -1;}

/*成员列表*/
.team_member_list{ width: 100%; background-color: #ffffff; }
.team_member_list>li{ position: relative; width: 100%; height: 60px;}
.team_member_list>li.check:after{ content: '\e605'; position: absolute; top: 18px; right: 15px; width: 16px; height: 12px; color: #1c77d2;}
.team_member_list>li>.avatar{display: inline-block; width: 75px; height: 100%; padding: 8px; vertical-align: top; text-align: center;}
.team_member_list>li>.avatar>img{ width: 40px; height: 40px; border: 1px solid transparent; border-radius: 50%;}
.team_member_list>li>.member_name{ display: inline-block; height: 100%; width: 300px; border-bottom:1px solid #f5f5f5;/*no*/}
.team_member_list>li>.member_name>span{line-height: 60px; color: #1d1d1d;font-size: 18px;}
/*横向成员列表*/
.across_member_list{ display: inline-block; vertical-align: top; padding: 10px 0; margin: 0;}
.across_member_list>li{ display: inline-block; vertical-align: top; width: 30px; height: 30px; margin-right: 5px;}
.across_member_list>li>img{ width: 100%; height: 100%; border: 1px solid transparent; border-radius: 50%;vertical-align: top;}

.full_page.warning{ margin: 240px 0 130px 0; text-align: center; color: #a9a9a9;font-size: 20px;}

/*新建任务*/
.newtask_page{position: relative;  width: 100%;  height: 100%;overflow: hidden;}
.newtask_page>ul{ width: 100%; margin-top: 21px; background-color: #ffffff;}
.newtask_page>ul>li{ width: 100%; height: 50px; }
.newtask_page>ul>li>input{ width: 100%; height: 50px; font-size: 18px; color:#131313;text-indent: 18px;}
.newtask_page>ul>li>input::-webkit-input-placeholder{ color:#939298; font-size: 18px; text-indent: 18px;}
.newtask_page .box_wrap { margin-top: 21px; padding-left: 18px; background-color: #ffffff;}

.jump_option_list{ width: 100%; background-color: #ffffff;}
.jump_option_list>li{ width: 100%; height: 50px; position: relative;}
.jump_option_list>li{border-bottom: 1px solid #f5f5f5;/*no*/}
.jump_option_list>li>.option{ height: 100%; width: 100%;}
.jump_option_list>li>.option+input[type=datetime-local]{ position: absolute; left: 0; top: 0; width: 1px; height: 1px;z-index: -1;}
.jump_option_list>li>.option>span{ line-height: 49px; font-size: 18px; color: #131313; }
.jump_option_list.small>li>.option>span{font-size: 12px;}
.jump_option_list.middle>li>.option>span{font-size: 16px;}
.jump_option_list>li>.right_block{ position: absolute; right: 0; top: 0; height: 100%; padding-right: 18px; overflow: hidden;}
.jump_option_list>li>.right_block>.icon{font-size: 14px; line-height: 50px; color: #c8c8cd;}
.jump_option_list>li>.right_block>.tip{ display: inline-block;vertical-align: top; max-width: 250px;overflow-x: auto; margin-right: 10px; font-size: 14px; line-height: 50px; color: #a5a5a5;  -webkit-overflow-scrolling:touch;}

/*增加项目成员*/
.add_member_page{ position: relative;  width: 100%;  height: 100%;overflow: hidden; font-size: 18px; color: #373737;}
.add_member_page>.scroll_box{ height: calc(100% - 45px); width: calc(100% + 10px); padding-right: 10px; overflow-y: scroll; -webkit-overflow-scrolling:touch;}
.add_member_page>.search_box{ width: 100%; height: 45px; padding: 8px;}
.add_member_page>.search_box>li{ width: 100%; height: 100%;   }
.add_member_page>.search_box>li>input{ width: 100%; height: 100%; border: 1px solid transparent;/*no*/ border-radius: 5px; text-align: center; color: #929297;font-size: 14px;}
.add_member_page>.search_box>li>input::-webkit-input-placeholder{ font-size: 14px; font-family:'normal_icon';}



/*默认项目*/
.default_project_page{position: relative;  width: 100%;  height: 100%;overflow: hidden;}
.default_project_page>.scroll_box{height: calc(100% - 45px); width: calc(100% + 10px); padding-right: 10px; overflow-y: scroll; -webkit-overflow-scrolling:touch;}
.default_project_page>.tab_box{ width:100%; height: 42px; margin: 1px 0 1px 0;padding: 7px 16px 7px 16px; background-color: #f8f8f8;}
.default_project_page>.tab_box>ul{width:100%; height: 100%; border: 1px solid #0076ff;/*no*/ border-radius: 5px; font-size:12px; }
.default_project_page>.tab_box>ul>li{ display:inline-block; width:33.33%; height: 100%; border-right: 1px solid #0076ff;/*no*/}
