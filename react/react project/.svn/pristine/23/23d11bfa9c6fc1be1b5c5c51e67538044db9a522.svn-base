/*--voice--*/
.voice_box{ margin-left:-6px; width:255px;}
.voice_box .tag{ width:200px; height:36px; display:inline-block; border:1px solid #a3d797; border-radius:5px; position:relative; background-color:#a3d797; margin-left:10px;}
.voice_box .tag:before{position: absolute; content: ''; width:0; height:0; padding: 0; left: -10px; top: 10px; border-width: 5px; border-style: solid; border-color: transparent; border-right-color: #a3d797;}
.voice_box .tag i{ color:#72a566;line-height:36px; margin-left:13px;}
.voice_box .tag audio{display: none;}
.voice_box .duration{ margin-left: 10px; line-height:36px; font-size:16px; color:#999; }

.voice_box.others .tag{ border-color:#eee; background-color:#f5f5f5;}
.voice_box.others .tag i{ color:#9e9e9e; line-height:36px; margin-left:13px;}
.voice_box.others .tag:before{ border-right-color: #f5f5f5;}

.video_box video{ width: 100%;}

