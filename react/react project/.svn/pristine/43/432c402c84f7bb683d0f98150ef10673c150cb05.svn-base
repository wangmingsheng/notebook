
module.exports = {
	getModule: function(type) {
		switch (type.toLowerCase()) {
			case 'mytask':
				return (require('./task/MyTask'));
				break;
            case 'project':
				return (require('./task/Project'));
                break;
            case 'create':
				return (require('./task/Create'));
                break;
			case 'createentry':
				return (require('./task/CreateEntry'));
				break;
			case 'partake':
				return (require('./task/Partake'));
				break;
			case 'detail':
				return (require('./task/Detail'));
                break;
			default :
				return (require('./task/MyTask'));
		}
	}
};