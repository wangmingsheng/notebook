@import "media.css";
@import "base.css";
@import 'stylus-px2rem/mixins'
@import 'stylus-px2rem/font-size'
@import 'stylus-px2rem/border'
@import 'stylus-px2rem/margin'
@import 'stylus-px2rem/padding'
@import 'stylus-px2rem/width'
@import 'stylus-px2rem/height'
@import 'stylus-px2rem/line-height'
.normal_icon{
    font-family:"normal_icon" !important;
    font-size:18px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}

/*我的任务*/
.wrap>.mytask_page>div{  width: 100%;  }
.wrap>.mytask_page>div>h4{ width: 100%;  height: 30px; overflow: hidden;text-indent: 18px;}
.wrap>.mytask_page>div>h4>span{ color: #9d9d9f; line-height: 30px; }
.wrap>.mytask_page>div>ul{ position: relative; width: 100%; background-color: #ffffff;overflow: hidden;}
.wrap>.mytask_page>div>ul>li{ position: relative; width: 515px; }
.wrap>.mytask_page>div>ul>li::after{ content: ''; position: absolute; right: 0; bottom: 0; width: 515px; border-bottom:1px solid #efeff4;}
.wrap>.mytask_page>div>ul>li>.check{ position: absolute; top: 0; left: 0; bottom: 0; width: 46px; }
.wrap>.mytask_page>div>ul>li>.check>input{ display: none; }
.wrap>.mytask_page>div>ul>li>.check>i{ display: inline-block; width: 19px; height: 20px; margin-left: 18px; margin-top: 14px; border: 1px solid #979797; border-radius: 5px; }
.wrap>.mytask_page>div>ul>li>.check>input:checked+i:after{content: '\e605';}
.wrap>.mytask_page>div>ul>li>.text{ display: inline-block; width: 329px; margin-left: 46px; margin-right: -0.5em; }
.wrap>.mytask_page>div>ul>li>.text>label{ display: inline-block; padding-top: 14px; padding-right: 22px; width: 100%;}
.wrap>.mytask_page>div>ul>li>.text>label>.top_block{ display: block; width: 100%;}
.wrap>.mytask_page>div>ul>li>.text>label>.top_block>.text{ font-size: 14px; line-height:20px; color:#474747; }
.wrap>.mytask_page>div>ul>li>.text>label>.top_block>.tip{ display: inline-block; height: 20px; padding: 0 12px 0 12px; margin-left: 10px; line-height: 22px; border-radius: 10px; font-size: 12px; text-align: center; }
.wrap>.mytask_page>div>ul>li>.text>label>.bottom_block{ display: block; width: 100%; }
.wrap>.mytask_page>div>ul>li>.text>label>.bottom_block>.logo{ font-size: 14px; line-height: 27px; color: #aaaaaa;}
.wrap>.mytask_page>div>ul>li>.option{ position: absolute; left: 375px; top: 0; width: 140px; height: 100%;}
.wrap>.mytask_page>div>ul>li>.option>input{ width: 70px; height: 100%;}
.wrap>.mytask_page>div>ul>li>.option>input:nth-child(1):after{content: '\e600'}
.wrap>.mytask_page>div>ul>li>.option>input:nth-child(2):after{content: '\e604'}

/*新建任务*/
.wrap>.newtask_page>ul{ width: 100%; padding-left: 18px; margin-top: 21px; background-color: #ffffff;}
.wrap>.newtask_page>ul>li{ width: 100%; height: 50px; }
.wrap>.newtask_page>ul>li:not(:nth-child(1)){border-top: 1px solid #efeff4;}
.wrap>.newtask_page>ul>li>input{ width: 100%; height: 50px; font-size: 18px; color:#939298;}
.wrap>.newtask_page>ul>li>input::-webkit-input-placeholder{ color:#939298; font-size: 18px;}
.wrap>.newtask_page>ul>li>.option{ line-height: 49px; font-size: 18px; }
.wrap>.newtask_page>ul>li>.right_block{ float: right; height: 100%; padding-right: 17px;}
.wrap>.newtask_page>ul>li>.right_block>.icon{ display: inline-block;  width: 10px; height: 16px; margin: 17px 0 0 17px; background: url("images/right_icon.png") no-repeat center;}
.wrap>.newtask_page>ul>li>.right_block>.tip{ font-size: 14px; line-height: 50px; color: #a5a5a5; }
.wrap>.newtask_page>ul>li>.right_block>input{ display: none; overflow: hidden;}
.wrap>.newtask_page>ul>li>.right_block>label{position: relative; display: inline-block; width: 51px; height: 31px; margin-top: 10px; }
.wrap>.newtask_page>ul>li>.right_block>label::before{ content: ''; position: absolute; left: 0; top: 0; width: 100%; height: 100%; background-color: #1f75d2; border-radius: 15px; }
.wrap>.newtask_page>ul>li>.right_block>input:checked+label::before{ background-color: #efeff4;}
.wrap>.newtask_page>ul>li>.right_block>label::after{ content: ''; position: absolute; width: 28px; height: 28px; top: 2px; right: 2px; border-radius: 100%; background-color: #ffffff; }
.wrap>.newtask_page>ul>li>.right_block>input:checked+label::after{ right: 20px; background-color: #1f75d2; }

/*增加项目成员*/
.wrap>.add_member_page{ font-size: 18px; color: #373737; overflow: hidden;}
.wrap>.add_member_page>.search_box{ width: 100%; height: 45px; padding: 8px;}
.wrap>.add_member_page>.search_box>li{ width: 100%; height: 100%;   }
.wrap>.add_member_page>.search_box>li>input{ width: 100%; height: 100%; border: 1px solid transparent; border-radius: 5px; text-align: center; color: #929297;}
.wrap>.add_member_page>.team_member_list{ width: 100%; background-color: #ffffff;}
.wrap>.add_member_page>.team_member_list>li{ position: relative; width: 100%; height: 60px;}
.wrap>.add_member_page>.team_member_list>li:after{ content: ''; position: absolute; top: 18px; right: 15px; width: 16px; height: 12px; }
.wrap>.add_member_page>.team_member_list>li>.avatar{display: inline-block; width: 74px; height: 100%; padding: 8px; vertical-align: top; text-align: center;}
.wrap>.add_member_page>.team_member_list>li>.avatar>image{}
.wrap>.add_member_page>.team_member_list>li>.member_name{ display: inline-block; height: 100%;}
.wrap>.add_member_page>.team_member_list>li>.member_name>span{line-height: 60px;}

