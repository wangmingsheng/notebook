@import "media.css";
@import "base.css";

/*上推对话框*/
.pushup_dialog{ position: absolute; width: 100%; height: 0; left: 0; bottom: 0; transition: height .5s;z-index: 10;background-color: #f2f2f2;}
.pushup_dialog.out{ height: 100%;}

/*-- CheckBox --*/
input[type=checkbox]{ display:none; overflow:hidden;}
input[type=checkbox] + label{ cursor:pointer; min-height:20px; min-width:20px; display:inline-block; position:relative; color:#333; text-indent:28px; font-size:16px; padding:0; line-height:20px;}
input[type=checkbox] + label:after{ position:absolute; left:0; top:0; width:20px; height:20px; border-radius:3px; border:1px solid #bbb; content:""; overflow:hidden; z-index:0;}
input[type=checkbox]:checked + label:before{ content:"\e605"; position:absolute; left:2px; top:0; z-index:1; text-indent:0; font-weight:bold; color:#90BBE9; font-size:16px;}
input[type=checkbox]:checked + label:after{ content:""; position:absolute; left:0px; top:0; z-index:0; width:20px; height:20px; background:#1F75D2; border-radius:3px; border:1px solid #1F75D2;}
input[type=checkbox]:indeterminate + label:before{ content:""; width:10px; height:2px; overflow:hidden; background:#888888; position:absolute; left:3px; top:45%;}
input[type=checkbox]:disabled + label{ cursor:default;}
input[type=checkbox]:disabled + label:before{ color:#cccccc;}
input[type=checkbox]:disabled + label:after{ border:1px solid #dddddd;}
 
.checkbox_radio{ display:inline-block;}
.checkbox_radio input[type=checkbox] + label:after{ border-radius:50%; }
.checkbox_radio input[type=checkbox]:checked + label:after{ border-radius:50%;}

.checkbox_round input[type=checkbox] + i:after{ border-radius:100%;}
/*SwitchBox*/
.switch_button{display: inline-block;}
.switch_button>input{ display: none; overflow: hidden;}
.switch_button>i{position: relative; display: inline-block; width: 51px; height: 31px; margin-top: 10px; }
.switch_button>i:before{ content: ''; position: absolute; left: 0; top: 0; width: 100%; height: 100%; background-color: #efeff4; border-radius: 15px; }
.switch_button>input:checked+i::before{ background-color: #1f75d2;}
.switch_button>i:after{ content: ''; position: absolute; width: 28px; height: 28px; top: 2px; right: 20px; border-radius: 100%; background-color: #ffffff; transition: right .3s;}
.switch_button>input:checked+i::after{ right: 2px;}

/*选择列表*/
.select_list{ width: 100%; }
.select_list>h4{ width: 100%;  height: 30px; overflow: hidden;text-indent: 18px;}
.select_list>h4>span{ color: #9d9d9f; line-height: 30px; }
.select_list>ul{ position: relative; width: 100%; background-color: #ffffff;overflow: hidden;}
.select_list>ul>li{ position: relative; width: 515px;}
.select_list>ul>li::after{ content: ''; position: absolute; left: 0; right: 0; bottom: 0; border-bottom:1px solid #efeff4;/*no*/}
.select_list.short_line>ul>li:nth-child(1)::after{ left: 18px;}
.select_list>ul>li>.check{ position: absolute; top: 0; left: 0; bottom: 0; width: 46px; padding: 18px 0 18px 18px;}

.select_list>ul>li>.text{ display: inline-block; width: 329px; padding: 12px 22px 12px 0; margin-left: 46px;}
.select_list>ul>li>.text>.top_block{ width: 100%;}
.select_list>ul>li>.text>.top_block>.text{ font-size: 14px; line-height:24px; color:#474747; }
.select_list>ul>li>.text>.top_block>.tip{ display: inline-block; height: 18px; padding: 0 8px 0 8px; margin-left: 10px; line-height: 18px; border-radius: 8px; font-size: 12px; text-align: center; color: #999999; background-color: #f2f2f2; }
.select_list>ul>li>.text>.top_block>.tip.later{ color: #ff8f78;background-color: #ffd4d3;}
.select_list>ul>li>.text>.bottom_block{ width: 100%; }
.select_list>ul>li>.text>.bottom_block>.brand{ font-size: 12px; line-height: 22px; color: #aaaaaa;}
.select_list>ul>li>.option{ position: absolute; left: 375px; top: 0; width: 140px; height: 100%;}
.select_list>ul>li>.option>input[type='button']{display: inline-block; width: 70px; height: 100%; font-size: 26px; background-color: #b79de1; color: #ffffff;}
.select_list>ul>li>.option>input[type='button']:nth-child(1){ background-color: #b79de1}
.select_list>ul>li>.option>input[type='button']:nth-child(2){ background-color: #82ced1}
.select_list>ul>li>.option>input:nth-child(3){ position: absolute; right: 370px; top: 0; width: 1px; height: 1px;z-index: -1;}

/*成员列表*/
.team_member_list{ width: 100%; height: calc(100% - 45px); background-color: #ffffff; overflow-y: scroll}
.team_member_list>li{ position: relative; width: 100%; height: 60px;}
.team_member_list>li.check:after{ content: '\e605'; position: absolute; top: 18px; right: 15px; width: 16px; height: 12px; color: #1c77d2;}
.team_member_list>li>.avatar{display: inline-block; width: 75px; height: 100%; padding: 8px; vertical-align: top; text-align: center;}
.team_member_list>li>.avatar>img{ width: 40px; height: 40px; border: 1px solid transparent; border-radius: 100%;}
.team_member_list>li>.member_name{ display: inline-block; height: 100%; width: 300px; border-bottom:1px solid #f5f5f5;/*no*/}
.team_member_list>li>.member_name>span{line-height: 60px;}
/*横向成员列表*/
.across_member_list{ height: 100%; padding: 12px;}
.across_member_list>li{ display: inline-block; width: 25px; height: 25px;}
.across_member_list>li>img{ width: 100%; height: 100%; border: 1px solid transparent; border-radius: 100%;}

/*新建任务*/
.newtask_page{position: relative;  width: 100%;  height: 100%;overflow: hidden;}
.newtask_page>ul{ width: 100%; margin-top: 21px; background-color: #ffffff;}
.newtask_page>ul>li{ width: 100%; height: 50px; }
.newtask_page>ul>li>input{ width: 100%; height: 50px; font-size: 18px; color:#939298;}
.newtask_page>ul>li>input::-webkit-input-placeholder{ color:#939298; font-size: 18px; text-indent: 18px;}

.jump_option_list{ width: 100%; padding-left: 18px; margin-top: 21px; background-color: #ffffff;}
.jump_option_list>li{ width: 100%; height: 50px; position: relative;}
.jump_option_list>li:not(:nth-child(1)){border-top: 1px solid #efeff4;/*no*/}
.jump_option_list>li>.option{ height: 100%; width: 100%;}
.jump_option_list>li>.option+input[type=datetime-local]{ position: absolute; left: 0; top: 0; width: 1px; height: 1px;z-index: -1;}
.jump_option_list>li>.option>span{ line-height: 49px; font-size: 18px; }
.jump_option_list>li>.option>.show{ display: inline-block;vertical-align: top;width: calc(100% - 100px); height: 100%; margin-left: 5px; overflow-x: scroll;overflow-y: hidden;}
.jump_option_list>li>.option>.show>span{text-align: center;font-size: 12px;line-height: 40px;}
.jump_option_list>li>.right_block{ position: absolute; right: 0; top: 0; height: 100%; padding-right: 17px;}
.jump_option_list>li>.right_block>.icon{ width: 10px; height: 16px; margin: 17px 0 0 17px; color: #c8c8cd;}
.jump_option_list>li>.right_block>.tip{ font-size: 14px; line-height: 50px; color: #a5a5a5; }

/*增加项目成员*/
.add_member_page{ position: relative;  width: 100%;  height: 100%;overflow: hidden; font-size: 18px; color: #373737;}
.add_member_page>.search_box{ width: 100%; height: 45px; padding: 8px;}
.add_member_page>.search_box>li{ width: 100%; height: 100%;   }
.add_member_page>.search_box>li>input{ width: 100%; height: 100%; border: 1px solid transparent;/*no*/ border-radius: 5px; text-align: center; color: #929297;font-size: 14px;}
.add_member_page>.search_box>li>input::-webkit-input-placeholder{ font-size: 14px;}



/*默认项目*/
.default_project_page{position: relative;  width: 100%;  height: 100%;overflow: hidden;}
.default_project_page>.tab_box{ width:100%; height: 42px;margin: 1px 0 1px 0;padding: 7px 16px 7px 16px; background-color: #f8f8f8;}
.default_project_page>.tab_box>ul{width:100%; height: 100%; border: 1px solid #0076ff;/*no*/ border-radius: 5px; font-size:12px; }
.default_project_page>.tab_box>ul>li{ display:inline-block; width:33.33%; height: 100%; border-right: 1px solid #0076ff;/*no*/}
.default_project_page>.tab_box>ul>li:last-child{ border-right-width: 0;}
.default_project_page>.tab_box>ul>li>label{ display:inline-block; width:100%; height: 100%; color: #0076ff;text-align: center; line-height: 27px;}
.default_project_page>.tab_box>ul>li.bkg>label{ background-color:#0076ff; color: #ffffff;}
.default_project_page input[name='tab_page']{ display:none; }
.default_project_page input[name='tab_page']:checked+ul{ display:block; }
.default_project_page input[name='tab_page']+ul{ display:none; }


/*我参与的任务*/
/*-- CheckBox --*/



p{ padding:0px; margin:0px;}
html{ -webkit-tap-highlight-color: rgba(0,0,0,0); font-family:"PingFang SC";}
body{ font-family:"PingFang SC";}
.mytask_join{ height:100%; overflow:auto;}
.mytask_join>h4{ height:30px; line-height:30px; font-weight:100; padding:0px 18px; background:#efeff4; color:#666;}
.mytask_join>h4 span{ float:left; font-size:12px; color:#4a4a4a;}
.mytask_join>h4 i{ float:right; font-size:12px; color:#c7c7cc;}
.mytask_join>ul{ padding-left:18px; background:#fff;}
.mytask_join>ul>li{ border-bottom:1px solid #f5f5f5; padding-right:18px;}
.mytask_join>ul .checkbox{ padding:13px 18px 13px 0px; display:block; overflow:hidden;}
.mytask_join>ul .checkbox .check{ float:left;}
.mytask_join>ul .checkbox .text{ float:right; width:311px; margin-left:8px;}
.mytask_join>ul .checkbox .text p{ font-size:16px; color:#000; line-height:22px;}
.mytask_join>ul .checkbox .text ul{ margin-top:9px;}
.mytask_join>ul .checkbox .text ul li div{ height:24px; position:relative;}
.mytask_join>ul .checkbox .text ul li div i{ font-size:15px; display:inline-block; margin-right:8px; line-height:24px; height:24px;}
.mytask_join>ul .checkbox .text ul li div label{ display:inline-block; font-size:13px; color:#4a4a4a; margin-right:8px; vertical-align:top; line-height:24px; height:24px;}
.mytask_join>ul .checkbox .text ul li div input{ position:absolute; left:0; top:6px; display:none;}
.mytask_join>ul .description{ padding:12px 18px 16px 0px;}
.mytask_join>ul>li>span{ font-size:13px; line-height:13px; color:#9b9b9b; display:block;}
.mytask_join>ul .description p{ font-size:14px; color:#333; line-height:16px; margin-top:7px;}
.mytask_join>ul .censor{ padding:14px 18px 13px 0px;}
.mytask_join>ul .censor ul{ padding-left:6px; margin-top:11px;}
.mytask_join>ul .censor ul li{ height:30px; padding:5px 0px; line-height:20px;}
.mytask_join>ul .title{ font-size:15px; color:#333; line-height:30px;}
.mytask_join>ul li .arrow{ line-height:30px; float:right; margin-left:6px; color:#c7c7cc; font-size:15px;}
.mytask_join>ul .join_member{ height:50px; padding:10px 18px 10px 0px;}
.mytask_join>ul .join_member span{ display:inline-block; font-size:15px; color:#030303;}
.mytask_join>ul .join_member ul{float:right; overflow:hidden;}
.mytask_join>ul .join_member ul li{ width:30px; height:30px; float:left; margin-right:6px;}
.mytask_join>ul .join_member ul li img{ width:30px;}
.mytask_join>ul .annex{ height:50px; padding:10px 18px 10px 0px;}
.mytask_join>ul .annex span{ display:inline-block;}
.mytask_join>ul .chat{ border:0;}
.mytask_join>ul .chat ul{ padding-top:19px;}
.mytask_join>ul .chat ul li{ overflow:hidden;}
.mytask_join>ul .chat ul li img{ float:left; width:40px; margin-right:14px;}
.mytask_join>ul .chat ul li .content{ float:left; padding-bottom:20px; width:282px;}
.mytask_join>ul .chat ul li .content .owner_info{ margin-bottom:6px;}
.mytask_join>ul .chat ul li .content .owner_info span{ font-size:17px; color:#095aab; height:24px; line-height:24px; overflow:hidden; margin-right:10px;}
.mytask_join>ul .chat ul li .content .owner_info label{ font-size:14px; color:#9c9c9c; height:24px; line-height:30px; display:inline-block; vertical-align:top; overflow:hidden; margin-right:5px;}
.mytask_join>ul .chat ul li .content p{ font-size:16px; color:#666; line-height:16px; font-family: "PingFang SC" arial;}
.mytask_join>ul .chat ul li .content .voice{ margin-left:-6px; width:255px;}
.mytask_join>ul .chat ul li .content .voice .tag{ width:200px; height:36px; display:inline-block; border:1px solid #a3d797; border-radius:5px; position:relative; background-color:#a3d797; margin-left:10px;}
.mytask_join>ul .chat ul li .content .voice .tag i{ color:#72a566;line-height:36px; margin-left:13px;}
.mytask_join>ul .chat ul li .content .voice .tag .arrow{ position:absolute; width:10px; height:10px; left:-17px; top:13px; }
.mytask_join>ul .chat ul li .content .voice .tag .arrow *{ display:block; border-width:5px; position:absolute; border-style:dashed solid dashed dashed; font-size:0; line-height:0; }
.mytask_join>ul .chat ul li .content .voice .tag .arrow em{ border-color:transparent #a3d797 transparent transparent;}
.mytask_join>ul .chat ul li .content .voice .tag .arrow span{ border-color:transparent #a3d797 transparent transparent; left:1px;}
.mytask_join>ul .chat ul li .content .voice .tag .duration{ display:inline-block; vertical-align:top; line-height:38px; font-size:16px; color:#999; position:absolute; right:-28px;}
.mytask_join>ul .chat ul li .content strong{ font-weight:100; font-size:16px; color:#989898; display:block; margin-bottom:15px;}
.mytask_join>ul .chat ul li .first_login .owner_info span{ color:#136500;}
.mytask_join>ul .chat ul li .others .voice .tag{ border-color:#eee; background-color:#f5f5f5;}
.mytask_join>ul .chat ul li .others .voice .tag i{ color:#9e9e9e; line-height:36px; margin-left:13px;}
.mytask_join>ul .chat ul li .others .voice .tag .arrow em{ border-color:transparent #eee transparent transparent;}
.mytask_join>ul .chat ul li .others .voice .tag .arrow span{ border-color:transparent #f5f5f5 transparent transparent;}
.mytask_join .chat_tool{ width:100%; height:48px; padding:7px 0px 7px 10px; position:fixed; left:0; bottom:0; z-index:999; background:#f6f6f6; border-top:1px solid #b2b2b2;}
.mytask_join .chat_tool i{ display:inline-block; width:25px; height:25px; border:1px solid #ddd; border-radius:50%; vertical-align:top; margin-top:4px; margin-right:3px;}
.mytask_join .chat_tool input{ display:inline-block; width:254px; height:32px; border:1px solid #ddd; outline:none; border-radius:4px; padding-left:5px; margin-right:3px;}
