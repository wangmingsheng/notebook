
module.exports = {
	getModule: function(type, callback) {
		switch (type.toLowerCase()) {
			case 'mytask':
				callback(require('./task/MyTask'));
				break;
            case 'project':
				callback(require('./task/Project'));
                break;
            case 'create':
				callback(require('./task/Create'));
                break;
			case 'createentry':
				callback(require('./task/CreateEntry'));
				break;
			case 'partake':
				callback(require('./task/Partake'));
			break;
			case 'detail':
				callback(require('./task/Detail'));
                break;
			default :
				callback(require('./task/MyTask'));
		}
	}
};