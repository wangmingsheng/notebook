@import "media.css";

@import "base.css";

/*上推对话框*/

.pushup_dialog {
  position: absolute;
  width: 100%;
  height: 0;
  left: 0;
  bottom: 0;
  transition: height .5s;
  z-index: 10;
  background-color: #f2f2f2;
}

.pushup_dialog.out {
  height: 100%;
}

/*-- CheckBox --*/

.checkbox input[type=checkbox] {
  display: none;
  overflow: hidden;
}

.checkbox input[type=checkbox] + i {
  cursor: pointer;
  min-height: 1rem;
  min-width: 1rem;
  display: inline-block;
  position: relative;
  color: #333;
  text-indent: 1.4rem;
  font-size: 0.8rem;
  padding: 0;
  line-height: 1rem;
}

.checkbox input[type=checkbox] + i:after {
  position: absolute;
  left: 0;
  top: 0;
  width: 1rem;
  height: 1rem;
  border-radius: 0.15rem;
  border: 0.05rem solid #bbb;
  content: "";
  overflow: hidden;
  z-index: 0;
}

.checkbox input[type=checkbox]:checked + i:before {
  content: "\e605";
  position: absolute;
  left: 0.1rem;
  top: 0;
  z-index: 1;
  text-indent: 0;
  font-weight: bold;
  color: #90BBE9;
  font-size: 0.8rem;
}

.checkbox input[type=checkbox]:checked + i:after {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
  width: 1rem;
  height: 1rem;
  background: #1F75D2;
  border-radius: 0.15rem;
  border: 0.05rem solid #1F75D2;
}

.checkbox input[type=checkbox]:indeterminate + i:before {
  content: "";
  width: 0.5rem;
  height: 0.1rem;
  overflow: hidden;
  background: #888888;
  position: absolute;
  left: 0.15rem;
  top: 45%;
}

.checkbox input[type=checkbox]:disabled + i {
  cursor: default;
}

.checkbox input[type=checkbox]:disabled + i:before {
  color: #cccccc;
}

.checkbox input[type=checkbox]:disabled + i:after {
  border: 0.05rem solid #dddddd;
}

.checkbox_radio {
  display: inline-block;
}

.checkbox_radio input[type=checkbox] + i:after {
  border-radius: 50%;
}

.checkbox_radio input[type=checkbox]:checked + i:after {
  border-radius: 50%;
}

.checkbox_round input[type=checkbox] + i:after {
  border-radius: 100%;
}

/*SwitchBox*/

.switch_button {
  display: inline-block;
}

.switch_button>input {
  display: none;
  overflow: hidden;
}

.switch_button>i {
  position: relative;
  display: inline-block;
  width: 2.55rem;
  height: 1.55rem;
  margin-top: 0.5rem;
}

.switch_button>i:before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #efeff4;
  border-radius: 0.75rem;
}

.switch_button>input:checked+i::before {
  background-color: #1f75d2;
}

.switch_button>i:after {
  content: '';
  position: absolute;
  width: 1.4rem;
  height: 1.4rem;
  top: 0.1rem;
  right: 1rem;
  border-radius: 100%;
  background-color: #ffffff;
  transition: right .3s;
}

.switch_button>input:checked+i::after {
  right: 0.1rem;
}

/*选择列表*/

.select_list {
  width: 100%;
}

.select_list>h4 {
  width: 100%;
  height: 1.5rem;
  overflow: hidden;
  text-indent: 0.9rem;
}

.select_list>h4>span {
  color: #9d9d9f;
  line-height: 1.5rem;
}

.select_list>ul {
  position: relative;
  width: 100%;
  background-color: #ffffff;
  overflow: hidden;
}

.select_list>ul>li {
  position: relative;
  width: 25.75rem;
}

.select_list>ul>li::after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  border-bottom: 1px solid #efeff4;
}

.select_list.short_line>ul>li:nth-child(1)::after {
  left: 0.9rem;
}

.select_list>ul>li>.check {
  display: inline-block;
  vertical-align: top;
  width: 2.3rem;
  padding: 0.6rem 0 0.6rem 0.9rem;
}

.select_list>ul>li>.text {
  display: inline-block;
  width: 16.45rem;
  padding: 0.5rem 1.1rem 0.25rem 0;
}

.select_list>ul>li>.text>.top_block {
  width: 100%;
}

.select_list>ul>li>.text>.top_block>.text {
  font-size: 0.7rem;
  color: #474747;
  line-height: 1.15rem;
}

.select_list>ul>li>.text>.top_block>.tip {
  display: inline-block;
  height: 0.9rem;
  padding: 0 0.4rem 0 0.4rem;
  margin: 0.15rem 0 0 0.5rem;
  line-height: 0.95rem;
  border-radius: 0.4rem;
  font-size: 0.6rem;
  text-align: center;
  color: #999999;
  background-color: #f2f2f2;
}

.select_list>ul>li>.text>.top_block>.tip.later {
  color: #ff8f78;
  background-color: #ffd4d3;
}

.select_list>ul>li>.text>.bottom_block {
  width: 100%;
}

.select_list>ul>li>.text>.bottom_block>.brand {
  font-size: 0.6rem;
  line-height: 1.1rem;
  color: #aaaaaa;
}

.select_list>ul>li>.option {
  position: absolute;
  left: 18.75rem;
  top: 0;
  width: 7rem;
  height: 100%;
}

.select_list>ul>li>.option>input[type='button'] {
  display: inline-block;
  width: 3.5rem;
  height: 100%;
  background-color: #b79de1;
  font-size: 1.3rem;
  color: #ffffff;
}

.select_list>ul>li>.option>input[type='button']:nth-child(1) {
  background-color: #b79de1;
}

.select_list>ul>li>.option>input[type='button']:nth-child(2) {
  background-color: #82ced1;
}

.select_list>ul>li>.option>input:nth-child(3) {
  position: absolute;
  right: 18.5rem;
  top: 0;
  width: 0.05rem;
  height: 0.05rem;
  z-index: -1;
}

/*成员列表*/

.team_member_list {
  width: 100%;
