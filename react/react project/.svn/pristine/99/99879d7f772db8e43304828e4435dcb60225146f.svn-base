  vertical-align: top;
  font-size: 0.7rem;
  line-height: 2.5rem;
  color: #a5a5a5;
  max-width: 13.25rem;
  overflow-x: scroll;
  -webkit-overflow-scrolling: touch;
}

/*增加项目成员*/

