}

.team_member_list>li {
  position: relative;
  width: 100%;
  height: 3rem;
}

.team_member_list>li.check:after {
  content: '\e605';
  position: absolute;
  top: 0.9rem;
  right: 0.75rem;
  width: 0.8rem;
  height: 0.6rem;
  color: #1c77d2;
}

.team_member_list>li>.avatar {
  display: inline-block;
  width: 3.75rem;
  height: 100%;
  padding: 0.4rem;
  vertical-align: top;
  text-align: center;
}

.team_member_list>li>.avatar>img {
  width: 2rem;
  height: 2rem;
  border: 0.05rem solid transparent;
  border-radius: 100%;
}

.team_member_list>li>.member_name {
  display: inline-block;
  height: 100%;
  width: 15rem;
  border-bottom: 1px solid #f5f5f5;
}

.team_member_list>li>.member_name>span {
  line-height: 3rem;
  color: #1d1d1d;
  font-size: 0.9rem;
}

/*横向成员列表*/

.across_member_list {
  display: inline-block;
  vertical-align: top;
  padding: 0.5rem 0;
  margin: 0;
}

.across_member_list>li {
  display: inline-block;
  width: 1.5rem;
  height: 1.5rem;
  margin-right: 0.25rem;
}

.across_member_list>li>img {
  width: 100%;
  height: 100%;
  border: 0.05rem solid transparent;
  border-radius: 100%;
  vertical-align: top;
}

/*新建任务*/

.newtask_page {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.newtask_page>ul {
  width: 100%;
  margin-top: 1.05rem;
  background-color: #ffffff;
}

.newtask_page>ul>li {
  width: 100%;
  height: 2.5rem;
}

.newtask_page>ul>li>input {
  width: 100%;
  height: 2.5rem;
  font-size: 0.9rem;
  color: #131313;
  text-indent: 0.9rem;
}

.newtask_page>ul>li>input::-webkit-input-placeholder {
  color: #939298;
  font-size: 0.9rem;
  text-indent: 0.9rem;
}

.jump_option_list {
  width: 100%;
  padding-left: 0.9rem;
  margin-top: 1.05rem;
  background-color: #ffffff;
}

.jump_option_list[data-padding="0"] {
  padding-left: 0;
}

.jump_option_list>li {
  width: 100%;
  height: 2.5rem;
  position: relative;
}

.jump_option_list>li:not(:nth-child(1)) {
  border-top: 1px solid #efeff4;
}

.jump_option_list>li>.option {
  height: 100%;
  width: 100%;
}

.jump_option_list>li>.option+input[type=datetime-local] {
  position: absolute;
  left: 0;
  top: 0;
  width: 0.05rem;
  height: 0.05rem;
  z-index: -1;
}

.jump_option_list>li>.option>span {
  line-height: 2.45rem;
  font-size: 0.9rem;
  color: #131313;
}

.jump_option_list>li>.option>.show {
  display: inline-block;
  vertical-align: top;
  width: calc(100% - 5rem);
  height: 100%;
  margin-left: 0.25rem;
  text-align: right;
  overflow-x: scroll;
  overflow-y: hidden;
  -webkit-overflow-scrolling: touch;
}

.jump_option_list>li>.option>.show>span {
  font-size: 0.6rem;
  line-height: 2.45rem;
}

.jump_option_list>li>.right_block {
  position: absolute;
  right: 0;
  top: 0;
  height: 100%;
  padding-right: 0.85rem;
}

.jump_option_list>li>.right_block>.icon {
  font-size: 0.8rem;
