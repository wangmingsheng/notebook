@font-face{ font-family:'normal_icon';
    src:url('../../fonts/normal_icon.eot');
    src:url('../../fonts/normal_icon.eot?#iefix') format('embedded-opentype'),
    url('../../fonts/normal_icon.woff') format('woff'),
    url('../../fonts/normal_icon.ttf') format('truetype'),
    url('../../fonts/normal_icon.svg#normal_icon') format('svg');
}

*{ box-sizing: border-box; font-family: 'normal_icon';}
*:after,*:before{box-sizing: border-box;}
html,body{  margin: 0;  padding: 0;  width: 100%;  height: 100%; -webkit-user-select: none; user-select: none; -webkit-tap-highlight-color: rgba(0,0,0,0);}
a, img { -webkit-touch-callout: none; /* 禁止长按链接与图片弹出菜单 */ }
ul{ margin: 0; padding: 0; list-style: none;}
i,image,input,button{ margin: 0; padding: 0; border: 0; outline: none; vertical-align: top;}
label,input[type="button"],input[type="submit"],input[type="reset"]{ -webkit-appearance: none; border-radius:0; }

h4{ margin: 0; padding: 0; font-size: 14px; font-weight: normal;}
i{ display: inline-block; font-style: normal; -webkit-font-smoothing: antialiased; -webkit-text-stroke-width: 0.2px;}
.wrap{ position: relative;  width: 100%;  height: 100%; background-color: #efeff4; overflow: hidden;}

.module_box{ position:absolute; top:0; right:0; bottom:0; left:0; overflow:hidden;}
.module_current{ position: absolute; width: 100%; height: 100%; left: 0; top:0; transition:left .4s;}
.module_next{ position:absolute; width:100%; height:100%; left:100%; top:0}